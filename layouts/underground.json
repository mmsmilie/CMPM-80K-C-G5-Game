    {
      "type": "BuiltinCommonInstructions::JsCode",
      "inlineCode": [
        "if(!runtimeScene.getGame().getVariables().get(\"shouldLoadUnderground\").getAsBoolean()){",
        "    runtimeScene.getGame().getVariables().get(\"shouldLoadUnderground\").setBoolean(true)",
        "",
        "    let x = runtimeScene.getGame().getVariables().get(\"undergroundSegmentX\").getAllChildrenArray() ",
        "    let y = runtimeScene.getGame().getVariables().get(\"undergroundSegmentY\").getAllChildrenArray()",
        "    let angles = runtimeScene.getGame().getVariables().get(\"undergroundSegmentAngles\").getAllChildrenArray()",
        "",
        "    for(let i=0;i<y.length;i++){",
        "        let newSegment = runtimeScene.createObject(\"VineSegment\")",
        "        newSegment.getVariables().get(\"hasSurfaced\").setBoolean(true)",
        "        newSegment.setX(x[i])",
        "        newSegment.setY(y[i])",
        "        newSegment.setAngle(angles[i])",
        "    }",
        "   ",
        "}",
        "",
        "",
        "const keyManager = runtimeScene.getGame().getInputManager(), S_KEY = 83, W_KEY = 87, A_KEY=65,D_KEY=68,SPACE_BAR=32",
        "const seg = objects[0]",
        "",
        "const lerp = (fromValue, toValue, dx) => gdjs.evtTools.common.lerp(fromValue,toValue,dx)",
        "const clamp = (number, lower, upper) => gdjs.evtTools.common.clamp(number, lower, upper)",
        "",
        "runtimeScene.getGame().getVariables().get(\"isSegmentsColliding\").setBoolean(false) ",
        "",
        "let objects1 = objects.filter(obj => obj.getVariables().get(\"hasSurfaced\").getAsBoolean() === false) //Only move the unsurfaced segments",
        "for(let i=1;i<objects1.length;i++){",
        "   if(objects1[i].getDistanceToObject(objects1[i-1]) > 80){ ",
        "        objects1[i].setX(lerp(objects1[i].getX(),objects1[i-1].getX(),0.05))",
        "        objects1[i].setY(lerp(objects1[i].getY(),objects1[i-1].getY(),0.05))",
        "        objects1[i].setAngle(lerp(objects1[i].getAngle(), objects1[i].getAngleToObject(objects1[i-1]),0.08))",
        "",
        "   } ",
        "",
        "   //Tangled handling: Check if head(seg) intersects with the other segments that aren't the first one behind the head",
        "   if(gdjs.RuntimeObject.collisionTest(seg, objects1[i]) && i!==1 && runtimeScene.getTimeManager().getTimeFromStart()>5000){ ",
        "        runtimeScene.getGame().getVariables().get(\"isSegmentsColliding\").setBoolean(true) ",
        "   } ",
        "}",
        "",
        "//Handle getting tangled/collision",
        "if(!runtimeScene.getGame().getVariables().get(\"isSegmentsColliding\").getAsBoolean()){",
        "     runtimeScene.getGame().getVariables().get(\"hasStoppedColliding\").setBoolean(true) ",
        "} else if(runtimeScene.getGame().getVariables().get(\"hasStoppedColliding\").getAsBoolean()){",
        "     seg.getVariables().get(\"polarForceMagnitude\").setNumber(seg.getVariables().get(\"polarForceMagnitude\").getAsNumber()*.75)",
        "     runtimeScene.getSoundManager().playSound(\"GrassSound.mp3\", false, 100, 1)",
        "     runtimeScene.getGame().getVariables().get(\"hasStoppedColliding\").setBoolean(false) ",
        "}",
        "",
        "//Keyboard stuff",
        "seg.clearForces()",
        "seg.addPolarForce(seg.getAngle(),seg.getVariables().get(\"polarForceMagnitude\").getAsNumber(),1)",
        "seg.rotateTowardPosition(keyManager.getCursorX(),keyManager.getCursorY(),seg.getVariables().get(\"angularVelocity\").getAsNumber())",
        "",
        "",
        "//Wait until the last segment exits some radius away from the spawn point, then create a new segment at the spawnpoint instead of using a timer",
        "let pivotX = runtimeScene.getGame().getVariables().get(\"pivotX\").getAsNumber(), pivotY=runtimeScene.getGame().getVariables().get(\"pivotY\").getAsNumber()",
        "if(objects[objects.length-1].getDistanceToPosition(pivotX,pivotY) > 100){",
        "    let newSegment = runtimeScene.createObject(\"VineSegment\")",
        "",
        "    //SPAWNPOINT/PIVOT coordinates",
        "    ",
        "    newSegment.setX(pivotX)",
        "    newSegment.setY(pivotY)",
        "    ",
        "}",
        "",
        "//Switch scene",
        "if(keyManager.isKeyPressed(D_KEY) && !runtimeScene.getVariables().get(\"isPaused\").getAsBoolean() && !runtimeScene.getGame().getVariables().get(\"isGameOver\").getAsBoolean()){",
        "    //Set global pivot position",
        "    runtimeScene.getGame().getVariables().get(\"pivotX\").setNumber(seg.getX())",
        "    runtimeScene.getGame().getVariables().get(\"pivotY\").setNumber(seg.getY())",
        "",
        "    //Clear position holding arrays",
        "    let x = runtimeScene.getGame().getVariables().get(\"undergroundSegmentX\")",
        "    let y=runtimeScene.getGame().getVariables().get(\"undergroundSegmentY\")",
        "    let angles=runtimeScene.getGame().getVariables().get(\"undergroundSegmentAngles\")",
        "",
        "    for(let i=0;i<objects.length;i++){ ",
        "        x.addChild(parseInt(i),objects[i].getX())",
        "        y.addChild(parseInt(i),objects[i].getY())",
        "        angles.addChild(parseInt(i),objects[i].getAngle())",
        "    }",
        "     for(let i=1;i<objects.length;i++){ ",
        "        objects[i].getVariables().get(\"hasSurfaced\").setBoolean(true)",
        "    }",
        "}",
        "",
        ""
      ],
