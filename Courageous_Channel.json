          "type": "BuiltinCommonInstructions::JsCode",
          "inlineCode": [
            "//runtimeScene.setBackgroundColor(100,100,240);",
            "",
            "//let dt = runtimeScene.getElapsedTime()",
            "",
            "if(runtimeScene.getTimeManager().isFirstFrame()) {",
            "    for(let i=0;i<5;i++){runtimeScene.createObject(\"Circle\")}",
            "}",
            "",
            "const keyManager = runtimeScene.getGame().getInputManager(), S_KEY = 83, W_KEY = 87, A_KEY=65,D_KEY=68,SPACE_BAR=32",
            "const seg = objects[0]",
            "",
            "const lerp = (fromValue, toValue, dx) => gdjs.evtTools.common.lerp(fromValue,toValue,dx)",
            "const clamp = (number, lower, upper) => gdjs.evtTools.common.clamp(number, lower, upper)",
            "",
            "for(let i=1;i<objects.length;i++){",
            "   if(objects[i].getDistanceToObject(objects[i-1]) > 45 ){ ",
            "       ",
            "        objects[i].setX(lerp(objects[i].getX(),objects[i-1].getX(),0.05))",
            "        objects[i].setY(lerp(objects[i].getY(),objects[i-1].getY(),0.05))",
            "        //todo growth DONE, food spawner, locking last segment, timer, ability icons, artwork/sound, collisions",
            "   } ",
            "",
            "   //Tangled handling: Check if head(seg) intersects with the other segments that aren't the first one behind the head",
            "   if(i !== 1 && seg.getDistanceToObject(objects[i]) < 20 && ",
            "      runtimeScene.getTimeManager().getTimeFromStart() > 10e3 &&",
            "      !runtimeScene.getGame().getVariables().get(\"isSegmentsColliding\").getAsBoolean()){ //20 is just an arbitrary pixel value that is close to the center of a segment",
            "        runtimeScene.getGame().getVariables().get(\"isSegmentsColliding\").setBoolean(true)",
            "        ",
            "   } else {",
            "        runtimeScene.getGame().getVariables().get(\"isSegmentsColliding\").setBoolean(false)",
            "   }",
            "}",
            "",
            "//Keyboard stuff",
            "seg.clearForces()",
            "seg.addPolarForce(seg.getAngle(),seg.getVariables().get(\"polarForceMagnitude\").getAsNumber(),1)",
            "seg.rotateTowardPosition(keyManager.getCursorX(),keyManager.getCursorY(),seg.getVariables().get(\"angularVelocity\").getAsNumber())",
            "",
            "",
            "//Wait until the last segment exits some radius away from the spawn point, then create a new segment at the spawnpoint instead of using a timer",
            "if(objects[objects.length-1].getX() > 50 || objects[objects.length-1].getX() > 80 ||objects[objects.length-1].getY() > 80){",
            "    let newSegment = runtimeScene.createObject(\"Circle\")",
            "",
            "    //SPAWNPOINT/PIVOT coordinates",
            "    newSegment.setX(0)",
            "    newSegment.setY(0)",
            "}",
            "",
            "",
            "//Camera",
            "//runtimeScene.getLayer(\"Base\").setCameraX(keyManager.getCursorX())",
            "//runtimeScene.getLayer(\"Base\").setCameraY(keyManager.getCursorY())",
            "",
            "",
            "",
            "",
